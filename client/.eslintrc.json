{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/no-danger": 0,
    "react/forbid-prop-types": 0,
    "prefer-destructuring": 0,
    "react/destructuring-assignment": 0,
    "no-underscore-dangle": 0
  }
}
